/// Helper to find substring position (returns -1 if not found)
let find_substring: fn(string, string) -> i32 = fn(haystack: string, needle: string) -> i32 {
    let hlen: i32 = std.string.length(haystack)
    let nlen: i32 = std.string.length(needle)
    if nlen > hlen {
        return -1
    }
    return find_substring_at(haystack, needle, 0, hlen - nlen + 1)
}

let find_substring_at: fn(string, string, i32, i32) -> i32 =
    fn(haystack: string, needle: string, pos: i32, limit: i32) -> i32 {
        if pos >= limit {
            return -1
        }
        let sub: string = std.string.substring(haystack, pos, pos + std.string.length(needle))
        if std.string.equals(sub, needle) {
            return pos
        }
        return find_substring_at(haystack, needle, pos + 1, limit)
    }

