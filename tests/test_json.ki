    let field: JsonField = JsonField { key: "nested", value: inner }
    let json: Json = json_object(Cons(field, Nil))

    // Act
    let output1: string = stringify(json)
    let output2: string = stringify(json)
    let output3: string = stringify(json)

    // Assert
    assert(std.string.equals(output1, output2), "first two outputs should match")
    assert(std.string.equals(output2, output3), "second and third outputs should match")
    return
}

// --- Parse Consistency Tests ---

let test_parse_consistency_valid_never_crashes: fn() -> void = fn() -> void {
    // Arrange - various valid JSON inputs
    let inputs: List[string] = Cons("null",
        Cons("true",
        Cons("false",
        Cons("0",
        Cons("-1",
        Cons("3.14",
        Cons("1e10",
        Cons("\"\"",
        Cons("\"hello\"",
        Cons("[]",
        Cons("[1,2,3]",
        Cons("{}",
        Cons("{\"a\":1}",
        Nil)))))))))))))

    // Act & Assert - all should parse successfully
    let check: fn(List[string]) -> void = fn(list: List[string]) -> void {
        match list {
            Nil => { return }
            Cons(input, rest) => {
                let result: Result[Json, JsonError] = parse(input)
                assert_ok[Json, JsonError](result, "valid JSON should always parse")
                check(rest)
            }
        }
    }
    check(inputs)
    return
}

let test_parse_consistency_invalid_never_crashes: fn() -> void = fn() -> void {
    // Arrange - various invalid JSON inputs
    let inputs: List[string] = Cons("",
        Cons("{",
        Cons("}",
        Cons("[",
        Cons("]",
        Cons("\"unterminated",
        Cons("nul",
        Cons("tru",
        Cons("fals",
        Cons("1.",
        Cons(".5",
        Cons("01",
        Cons("{\"a\"}",
        Nil)))))))))))))

    // Act & Assert - all should return errors (not crash)
    let check: fn(List[string]) -> void = fn(list: List[string]) -> void {
        match list {
            Nil => { return }
            Cons(input, rest) => {
                let result: Result[Json, JsonError] = parse(input)
                // We just want to verify it doesn't crash - error is expected
                match result {
                    Ok(_) => { }  // Some edge cases might parse unexpectedly, that's ok
                    Err(_) => { }
                }
                check(rest)
            }
        }
    }
    check(inputs)
    return
}

