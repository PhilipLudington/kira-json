import src.json.{ Json, JNull, JBool, JNumber, JString, JArray, JObject, JsonField, make_field, stringify }
import std.list.{ Cons, Nil }

effect fn main() -> IO[void] {
    let field: JsonField = make_field("name", JString("Alice"))
    let person: Json = JObject(Cons(field, Nil))
    std.io.println(stringify(person))
    return
}
