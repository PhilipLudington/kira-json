import src.json.types.{ Json, JNull, JBool, JNumber, JString, JArray, JsonField }
import src.json.serializer.{ stringify }
import src.json.builder.{ json_object }
import std.list.{ Cons, Nil }

effect fn main() -> IO[void] {
    let field: JsonField = JsonField { key: "name", value: JString("Alice") }
    let person: Json = json_object(Cons(field, Nil))
    std.io.println(stringify(person))
    return
}
